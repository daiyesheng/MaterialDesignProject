<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidwanga.serenitynanian.serenityproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ToolbarActivity" />
        <activity android:name=".BottomSheetDialogActivity" />
        <activity android:name=".CoordinatorLayoutActivity" />
        <activity android:name=".CollapsingToolbarLayoutActivity" />
        <activity android:name=".SwipeDismissBehaviorActivity" />
        <activity android:name=".TextInputActivity" />
        <activity android:name=".SnackBarActivity" />
        <activity android:name=".BottomDialogActivity" />
        <activity android:name=".BottomDialogFragmentActivity" />
        <activity android:name=".BehaviorActivity" />
        <activity android:name=".FloatActionButtonActivity" />
        <activity android:name=".CardViewActivity" />
        <activity android:name=".TabLayoutActivity"></activity>
    </application>

</manifest>